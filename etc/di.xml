<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
	<type name="Magento\Framework\Console\CommandList">
		<arguments>
			<argument name="commands" xsi:type="array">
				<item name="customer_import" xsi:type="object">Augustash\CustomerImport\Console\Command\CustomerImportCommand</item>
                <item name="customer_address_import" xsi:type="object">Augustash\CustomerImport\Console\Command\CustomerAddressImportCommand</item>
                <item name="stored_card_import" xsi:type="object">Augustash\CustomerImport\Console\Command\CustomerCardImportCommand</item>
			</argument>
